                    <CommandText>SELECT o.OrderID, o.CustomerID, o.OrderDate, o.RequiredDate, o.Freight, DATEDIFF(day, o.OrderDate, o.RequiredDate) AS DelaiLivraison, e.LastName + ', ' + e.FirstName AS Vendeur
